{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "log.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:8081"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MyFaceApi": "Server=(localdb)\\MSSQLLocalDB; Database=MyFaceApiv2;Trusted_Connection=True;MultipleActiveResultSets=True",
    "MyFaceOnlineUsers": "Server=(localdb)\\MSSQLLocalDB; Database=MyFacev2OnlineUsers;Trusted_Connection=True;MultipleActiveResultSets=True"
  },
  "IdentityServerConfiguration": {
    "IdentityServerUri": "https://localhost:44345/",
    "AudienceName": "MyFaceApiV2"
  },
  "Path": {
    "Images": "C:\\Users\\lenovo\\source\\repos\\MyFaceApi\\Images"
  }
}
